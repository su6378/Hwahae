<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:scrollbars="none"
    android:background="#EBEFF2">



    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/register_bottom_appbar"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:padding="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="가입경로"
            android:textColor="@color/black"
            android:textSize="18sp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/route_cancel"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="36dp"
            android:layout_height="36dp"
            android:src="@drawable/ic_cancel"
            android:tint="@color/black"
            app:layout_constraintRight_toRightOf="parent"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/friendsLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@id/register_bottom_appbar"
        android:paddingVertical="10dp"
        android:layout_marginTop="10dp">

        <TextView
            android:id="@+id/friends_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:text="친구, 지인의 추천(오프라인)"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/friends_check"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:visibility="invisible"
            android:src="@drawable/ic_check"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:tint="@color/splash_background"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="10dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/youtubeLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@id/friendsLayout"
        android:paddingVertical="10dp"
        android:layout_marginTop="1dp">

        <TextView
            android:id="@+id/youtube_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:text="유튜브 광고"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/youtube_check"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:visibility="invisible"
            android:src="@drawable/ic_check"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:tint="@color/splash_background"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="10dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/officialSNSLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@id/youtubeLayout"
        android:paddingVertical="10dp"
        android:layout_marginTop="1dp">

        <TextView
            android:id="@+id/officialSNS_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:text="화해 공식 SNS 채널"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/officialSNS_check"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:visibility="invisible"
            android:src="@drawable/ic_check"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:tint="@color/splash_background"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="10dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/etcLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@id/officialSNSLayout"
        android:paddingVertical="10dp"
        android:layout_marginTop="1dp">

        <TextView
            android:id="@+id/etc_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:text="기타"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/etc_check"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:visibility="invisible"
            android:src="@drawable/ic_check"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:tint="@color/splash_background"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="10dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/appStoreLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@id/etcLayout"
        android:paddingVertical="10dp"
        android:layout_marginTop="1dp">

        <TextView
            android:id="@+id/appStore_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:text="앱스토어 (검색, 카테고리, 인기순위 등)"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/appStore_check"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:visibility="invisible"
            android:src="@drawable/ic_check"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:tint="@color/splash_background"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="10dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/facebookADLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@id/appStoreLayout"
        android:paddingVertical="10dp"
        android:layout_marginTop="1dp">

        <TextView
            android:id="@+id/facebookAD_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:text="페이스북, 인스타그램 광고"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/facebookAD_check"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:visibility="invisible"
            android:src="@drawable/ic_check"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:tint="@color/splash_background"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="10dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cafeBlogLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@id/facebookADLayout"
        android:paddingVertical="10dp"
        android:layout_marginTop="1dp">

        <TextView
            android:id="@+id/cafeBlog_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:text="커뮤니티, 카페, 블로그"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/cafeBlog_check"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:visibility="invisible"
            android:src="@drawable/ic_check"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:tint="@color/splash_background"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="10dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/elevatorLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@id/cafeBlogLayout"
        android:paddingVertical="10dp"
        android:layout_marginTop="1dp">

        <TextView
            android:id="@+id/elevator_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:text="엘리베이터 TV"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/elevator_check"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:visibility="invisible"
            android:src="@drawable/ic_check"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:tint="@color/splash_background"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="10dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/newsLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@id/elevatorLayout"
        android:paddingVertical="10dp"
        android:layout_marginTop="1dp">

        <TextView
            android:id="@+id/news_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:text="뉴스, 매거진, 기사"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/news_check"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:visibility="invisible"
            android:src="@drawable/ic_check"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:tint="@color/splash_background"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="10dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/onlineBannerLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        app:layout_constraintTop_toBottomOf="@id/newsLayout"
        android:paddingVertical="10dp"
        android:layout_marginTop="1dp">

        <TextView
            android:id="@+id/onlineBanner_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginLeft="10dp"
            android:textColor="@color/black"
            android:text="온라인 배너(웹사이트, 타 어플 등)"/>

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/onlineBanner_check"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:visibility="invisible"
            android:src="@drawable/ic_check"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:tint="@color/splash_background"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginRight="10dp"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:background="#EBEFF2"
        app:layout_constraintTop_toBottomOf="@id/onlineBannerLayout"
        />







</androidx.constraintlayout.widget.ConstraintLayout>