<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="LoginTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/dimgray</item>
        <item name="colorPrimaryDark">@android:color/transparent</item>
        <item name="colorAccent">@color/dimgray</item>
    </style>

    <style name="FindTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/dimgray</item>
        <item name="colorPrimaryDark">@android:color/transparent</item>
        <item name="colorAccent">@color/dimgray</item>
        <item name="md_color_title">@color/black</item>
        <item name="md_color_content">@color/black</item>
        <item name="md_color_button_text">@color/black</item>
    </style>

    <style name="MainTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/dimgray</item>
        <item name="colorAccent">@color/white</item>
    </style>

    <style name="BottomNavigation" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="itemTextAppearance">@style/BottomNavigationText</item>
        <item name="itemTextColor">@color/black</item>
    </style>

    <style name="BottomNavigationText" parent="TextAppearance.AppCompat">
        <item name="android:textSize">14sp</item>
        <item name="itemTextColor">@color/black</item>
    </style>




</resources>